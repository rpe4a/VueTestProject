{
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "html"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "no-debugger": 0,
        "no-console": 0,
        "new-cap": 0,
        "strict": 0,
        "no-underscore-dangle": 0,
        // "no-use-before-define": 0,
        "no-var": 1,
        "no-empty": 1,
        "no-unexpected-multiline": 1,
        "eqeqeq": 1,
        "block-scoped-var": 1,
        "class-methods-use-this": 1,
        "no-empty-function": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-eval": 2,
        "no-implied-eval": 2,
        "no-multi-spaces": 1,
        "no-lone-blocks": 1,
        "no-multi-str": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "one-var": 1,
        "operator-linebreak": [1, "none"],
        "no-redeclare": 2,
        "no-plusplus": 2,
        "no-unneeded-ternary": 2,
        "no-return-assign": 1,
        "no-self-assign": 1,
        "no-whitespace-before-property": 1,
        "func-call-spacing": 1,
        "keyword-spacing": 1,
        "consistent-this": 1,
        "no-self-compare": 1,
        "no-new-object": 2,
        "no-throw-literal": 1,
        "newline-after-var": 1,
        "no-inline-comments": 1,
        "vars-on-top": 1,
        "new-parens": 1,
        "init-declarations": [2, "always"],
        "wrap-iife": 1,
        "no-with": 2,
        "array-bracket-spacing": 1,
        "block-spacing": 1,
        "brace-style": [1, "1tbs"],
        "comma-spacing": 1,
        "comma-style": 1,
        "semi-spacing": 1,
        "template-curly-spacing": 1,
        "no-useless-constructor": 1,
        "no-useless-computed-key": 1,
        "no-this-before-super": 2,
        "prefer-spread": 1,
        "prefer-template": 1,
        "rest-spread-spacing": 2,
        "no-dupe-class-members": 2,
        "computed-property-spacing": 1,
        "no-use-before-define": 2,
        "no-duplicate-imports": 2,
        "no-const-assign": 2,
        "eol-last": 0,
        "template-tag-spacing": ["error", "always"],
        "semi": 0,
        "quotes": [1, "single"],
        "jsx-quotes": [1, "prefer-single"],
        "no-unused-vars": 1
    }
}